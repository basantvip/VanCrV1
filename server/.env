# Copy this file to .env and fill values
# SMTP settings for your mail provider (e.g. Gmail, SendGrid SMTP, Mailgun SMTP)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your_smtp_username
SMTP_PASS=your_smtp_password

# Optional: the email address that will receive contact messages (default: basant.vip@gmail.com)
RECIPIENT_EMAIL=basant.vip@gmail.com

# Optional: from address to use in the outgoing email
FROM_EMAIL=contact@yourdomain.com

# Port the server will listen on
PORT=3000
